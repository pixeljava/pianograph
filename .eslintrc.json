{
    "env": {
      "browser": true,
      "es6": true,
      "node": true
      "global": "$"
    },
    "extends": ["airbnb-base"],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 9,
      "sourceType": "module"
    },
    "rules": {
      "indent": ["warn", 2],
      "linebreak-style": ["error", "unix"],
      "quotes": ["error", "single"],
      "semi": ["error", "always"],
      "no-console": 0,
      "comma-dangle": [0],
      "arrow-parens": [0],
      "object-curly-spacing": ["warn", "always"],
      "array-bracket-spacing": ["warn", "always"],
      "import/prefer-default-export": [0]
    }
  }
